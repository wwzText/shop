<view class="time-bar data-v-3dd3c30e"><view class="{{['app-pt-time data-v-3dd3c30e',theme==='a'?'default-back':theme+'-m-back '+theme]}}"><image class="app-image data-v-3dd3c30e" src="../image/time-bar.png"></image><view class="app-time-content data-v-3dd3c30e"><view class="app-name data-v-3dd3c30e">{{start_begin}}</view><view class="app-time-info dir-left-nowrap cross-center data-v-3dd3c30e"><text class="app-value data-v-3dd3c30e">{{day}}</text><text class="app-degree data-v-3dd3c30e">天</text><text class="app-value data-v-3dd3c30e">{{hour}}</text><text class="app-degree data-v-3dd3c30e">时</text><text class="app-value data-v-3dd3c30e">{{minute}}</text><text class="app-degree data-v-3dd3c30e">分</text><text class="app-value data-v-3dd3c30e">{{second}}</text><text class="app-degree data-v-3dd3c30e">秒</text></view></view></view></view>