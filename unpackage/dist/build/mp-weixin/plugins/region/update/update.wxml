<app-layout vue-id="908a6bd6-1" class="data-v-150ccad6" bind:__l="__l" vue-slots="{{['default']}}"><image class="bg data-v-150ccad6" src="{{region.detail}}"></image><view class="info data-v-150ccad6"><image class="avatar data-v-150ccad6" src="{{detail.avatar}}"></image><block wx:if="{{detail.level_up_log.status==1}}"><image class="success-img data-v-150ccad6" src="{{bonus.success}}"></image></block><block wx:if="{{detail.level_up_log.status==1}}"><view class="update main-between cross-center data-v-150ccad6"><text class="data-v-150ccad6">{{"恭喜您已升级成为"+detail.province_text+'的'}}<block wx:if="{{detail.level==1}}"><text class="data-v-150ccad6">省代理</text></block><block wx:if="{{detail.level==2}}"><text class="data-v-150ccad6">市代理</text></block></text></view></block><block wx:if="{{detail.is_up==1}}"><view class="update main-between cross-center data-v-150ccad6"><block wx:if="{{detail.level_up_log.status==0}}"><view class="data-v-150ccad6">{{"正在申请成为"+detail.province_text+"的"+detail.level_up.level_desc}}</view></block><block wx:else><view class="data-v-150ccad6">{{"可升级为"+detail.province_text+"的"+detail.level_up.level_desc}}</view></block><view data-event-opts="{{[['tap',[['toUpdate',['$event']]]]]}}" class="update-btn data-v-150ccad6" bindtap="__e">{{detail.level_up_log.status==0?'去查看':'去升级'}}</view></view></block><view class="title main-center cross-center data-v-150ccad6"><image src="./../image/title-left.png" class="data-v-150ccad6"></image><view class="data-v-150ccad6">代理级别</view><image src="./../image/title-right.png" class="data-v-150ccad6"></image></view><view class="level-info data-v-150ccad6"><block wx:if="{{detail.level==1}}"><text class="data-v-150ccad6">省代理</text></block><block wx:if="{{detail.level==2}}"><text class="data-v-150ccad6">市代理</text></block><block wx:if="{{detail.level==3}}"><text class="data-v-150ccad6">区/县代理</text></block><text class="placeholder data-v-150ccad6">|</text><block wx:if="{{detail.bonus_rate}}"><text class="data-v-150ccad6">{{(setting.form.rate?setting.form.rate:'分红比例')+detail.bonus_rate+"%"}}</text></block></view><view class="title main-center cross-center data-v-150ccad6"><image src="./../image/title-left.png" class="data-v-150ccad6"></image><view class="data-v-150ccad6">代理地区</view><image src="./../image/title-right.png" class="data-v-150ccad6"></image></view><view class="data-v-150ccad6"><text class="data-v-150ccad6">{{detail.province_text}}</text><block wx:if="{{detail.level>2&&detail.city_text}}"><text class="data-v-150ccad6"><text class="placeholder data-v-150ccad6">-</text>{{''+detail.city_text+''}}</text></block></view><block wx:if="{{detail.level>1&&!(detail.relation.length==1&&detail.relation[0].name==detail.province_text)}}"><view class="relation-list data-v-150ccad6"><block wx:for="{{detail.relation}}" wx:for-item="item" wx:for-index="index" wx:key="*this"><text class="data-v-150ccad6"><block wx:if="{{index>0}}"><text class="placeholder data-v-150ccad6">/</text></block>{{''+item.name+''}}</text></block></view></block></view></app-layout>